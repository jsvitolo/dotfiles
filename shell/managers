# PATHS
export RBENV_ROOT="$HOME/.rbenv"
export EXENV_ROOT="$HOME/.exenv"
export PATH="$RBENV_ROOT/bin:$EXENV_ROOT/bin:$PATH"

# Google Cloud

# The next line updates PATH for the Google Cloud SDK.
source '/Users/jonesvitolo/Projects/google-cloud-sdk/path.zsh.inc'

# The next line enables shell command completion for gcloud.
source '/Users/jonesvitolo/Projects/google-cloud-sdk/completion.zsh.inc'


# RBENV
which rbenv > /dev/null && eval "$(rbenv init -)"

# EXENV
which exenv > /dev/null && eval "$(exenv init -)"

# NVM
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"


